<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" spacing="30.0" style="-fx-background-color: lightgray;" styleClass="root" stylesheets="@graph-style.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.controllers.WinController">
   <children>
      <Group>
         <children>
            <Text layoutX="48.0" layoutY="9.0" strokeType="OUTSIDE" strokeWidth="0.0" text="You have completed the escape room." />
            <Text layoutY="26.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Thanks for playing and we hope you learned something!" />
         </children>
      </Group>
      <Button mnemonicParsing="false" onAction="#handleCloseButton" text="Close" />
   </children>
</VBox>
